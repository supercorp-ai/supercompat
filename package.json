{
  "name": "@supercorp/supercompat",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run build --force -- --watch",
    "lint": "turbo lint",
    "lint:ts": "tsc -p packages/supercompat/tsconfig.json --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "npx tsx --tsconfig tsconfig.test.json --test packages/supercompat/tests/*.test.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@swc/core": "^1.10.1",
    "@types/node": "^22.15.18",
    "https-proxy-agent": "^7.0.6",
    "undici": "^7.15.0",
    "prettier": "^3.4.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "turbo": "^2.3.3",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {}
}
